def Take_input():
    word = input()
    c = int(input())
    r = int(input())
    matrix = []
    for _ in range(c):
        matrix.append(list(input().split()))
    return word,c,r,matrix

word,c,r,matrix = Take_input()
print(word,c,r,matrix)